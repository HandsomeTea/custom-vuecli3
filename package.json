{
  "name": "custom-vuecli3",
  "version": "1.0.0",
  "description": "custom code with vue-cli 3",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "build": "vue-cli-service build --modern",
    "build-test": "vue-cli-service build -build-test",
    "lint": "vue-cli-service lint",
    "webpack-rules": "vue inspect --rules > webpack.default.rules.txt",
    "webpack-plugins": "vue inspect --plugins > webpack.default.plugins.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/custom-vuecli3.git"
  },
  "keywords": [
    "vue=cli3",
    "typecsript"
  ],
  "author": "handsometea.coco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/custom-vuecli3/issues"
  },
  "homepage": "https://github.com/HandsomeTea/custom-vuecli3#readme",
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.1-beta.24",
    "vant": "^3.0.3",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.0.0-rc.1",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^4.3.7",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.3",
    "prettier": "^1.19.1",
    "style-resources-loader": "^1.4.1",
    "typescript": "~3.9.3",
    "vuex-class": "^0.3.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "./**/*.{js,ts,vue}": [
      "npm run lint"
    ]
  }
}
