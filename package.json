{
  "name": "custom-vuecli3",
  "version": "1.0.0",
  "description": "custom code with vue-cli 3",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "build": "vue-cli-service build",
    "build-test": "vue-cli-service build -build-test",
    "upgrade": "./node_modules/.bin/npm-check -u",
    "lint": "vue-cli-service lint",
    "webpack-rules": "vue inspect --rules > webpack.default.rules.txt",
    "webpack-plugins": "vue inspect --plugins > webpack.default.plugins.txt",
    "husky-init": "husky install",
    "husky-hook": "husky add .husky/pro-commit \"npm run lint\" && husky add .husky/pro-push \"npm run lint\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeTea/custom-vuecli3.git"
  },
  "keywords": [
    "vue-cli3",
    "typecsript"
  ],
  "author": "handsometea.coco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HandsomeTea/custom-vuecli3/issues"
  },
  "homepage": "https://github.com/HandsomeTea/custom-vuecli3#readme",
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "element-plus": "^1.0.1-beta.24",
    "vant": "^3.0.10",
    "vue": "^3.0.7",
    "vue-i18n": "^9.0.0-rc.1",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.7",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "style-resources-loader": "^1.4.1",
    "typescript": "^4.2.3",
    "webpack-merge": "^5.7.3"
  },
  "lint-staged": {
    "./**/*.{js,ts,vue}": [
      "npm run lint"
    ]
  }
}
